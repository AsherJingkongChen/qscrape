{
  "name": "qscrape",
  "version": "0.0.0",
  "author": {
    "name": "Asher Jingkong Chen"
  },
  "bin": "./dist/bin.cjs",
  "description": "Easily scrape web pages",
  "dependencies": {
    "inquirer": "^9.2.15",
    "jsdom": "^24.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20",
    "@vitest/coverage-istanbul": "^1.2.2",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  },
  "exports": {
    ".": {
      "default": "./dist/lib.js",
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    },
    "./bin": {
      "default": "./dist/bin.js",
      "import": "./dist/bin.js",
      "require": "./dist/bin.cjs",
      "types": "./dist/bin.d.ts"
    },
    "./lib": {
      "default": "./dist/lib.js",
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "homepage": "https://asherjingkongchen.github.io/qscrape/",
  "keywords": [
    "scrape",
    "spider",
    "web"
  ],
  "license": "MIT",
  "main": "./dist/lib.cjs",
  "module": "./dist/lib.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AsherJingkongChen/qscrape.git"
  },
  "scripts": {
    "build": "run-p _build check format",
    "_build": "vite build",
    "check": "tsc",
    "dev-build": "vite build --mode development",
    "dev-test": "vitest",
    "format": "prettier --write .",
    "test": "vitest --run"
  },
  "type": "module",
  "types": "./dist/lib.d.ts"
}
